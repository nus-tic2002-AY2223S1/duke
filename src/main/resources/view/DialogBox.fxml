<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="800.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
    <VBox prefHeight="0.0" prefWidth="100.0"/>
    <Label id="meta" fx:id="meta" alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Label">
        <HBox.margin>
            <Insets left="10.0" right="10.0"/>
        </HBox.margin>
    </Label>
    <Label id="dialog1" fx:id="dialog" alignment="TOP_CENTER" contentDisplay="CENTER" text="Label" wrapText="true">
        <font>
            <Font size="14.0"/>
        </font>
        <padding>
            <Insets right="10.0"/>
        </padding>
    </Label>
    <ImageView id="displayPicture" fx:id="displayPicture" fitHeight="65.0" fitWidth="65.0" pickOnBounds="true"
               preserveRatio="true">
        <HBox.margin>
            <Insets left="22.0" right="10.0"/>
        </HBox.margin>
    </ImageView>
    <stylesheets>
        <URL value="@light.css"/>
    </stylesheets>
</fx:root>
